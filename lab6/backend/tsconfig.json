{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noUnusedLocals": true,
        "sourceMap": true,
        "declaration": false,
        "pretty": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
}
